{
  "name": "@xchainjs/xchain-vechain",
  "version": "0.1.0",
  "description": "Custom Vechain client and utilities used by XChainJS clients",
  "keywords": [
    "XChain",
    "Vechain"
  ],
  "author": "XChain",
  "homepage": "https://github.com/xchainjs/xchainjs-lib",
